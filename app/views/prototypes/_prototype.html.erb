<div class="card">
<%=link_to image_tag(prototype.image, method: :get, class: 'card__img'), prototype_path(prototype.id)if prototype.image.attached? %>

  <div class="card__body">
    <%= link_to prototype.prototype_name, prototype_path(prototype.id), method: :get, class: :card__title%>

    <p class="card__summary">
    <%= link_to prototype.catchcopy, method: :get %>
    </p>
    <%= link_to prototype.user.name, user_path(prototype.user_id), method: :get, class: :card__user %>
 
  </div>
</div>


